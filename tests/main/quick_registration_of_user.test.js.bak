const request = require('supertest');
const app = require('../../../app')
const quick_registration = require('../../../routes/register/quick_registration_of_user')
const con = require('../../../config/db.js');

describe('Quick Registration of users', () => {
    beforeAll(() => {
        con.connect();
    });

    describe("GET / ", () => {
        test("It should respond with an array of students", async () => {
            const body = {
                email: "studentmega@gmail.com",
                name: "Mega Ong",
                user_type: 0,
                user_status: 0
            }
            const response = await (request(quick_registration).post("/api/quickregistration").send(body));
            expect(response.status).toBe(200);
        });
    });
})
